<% if (page.prev || page.next){ %>
  <ul class="pager">
    <% if (page.prev){ %>
      <li class="prev">
        <a href="<%- url_for(page.prev_link) %>">
          <%= __('prev') %>
        </a>
      </li>
    <% } %>

    <% if (page.next){ %>
      <li class="next">
        <a href="<%- url_for(page.next_link) %>">
          <%= __('next') %>
        </a>
      </li>
    <% } %>
  </ul>
<% } %>
